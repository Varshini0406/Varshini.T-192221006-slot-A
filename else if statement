#include <stdio.h>
int main() 
{
   int a,b,c;
   printf("Enter 1st value:");
   scanf("%d",&a);
   printf("Enter 2nd value:");
   scanf("%d",&b);
   printf("Enter 3rd value:");
   scanf("%d",&c);
   if(a>b)
   {
      printf("A is greater");
   }
   else if(a>c)
   {
       printf("A is greater");
   }
   else if(b>c)
   {
       printf("B is greaterC");
   }
   else
   {
       printf("C is great");
   }
return 0;
}   
